{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "semicolon": [false, "always"],
        "quotemark": [true, "single"],
        "arrow-parens": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}